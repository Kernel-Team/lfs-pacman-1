pkgname=gdbm
pkgver=1.13
pkgrel=1
pkgdesc="GNU database library"
arch=('x86_64')
url="https://www.gnu.org/software/gdbm/"
license=('GPL3')
#depends=()
source=("https://ftp.gnu.org/gnu/gdbm/gdbm-${pkgver}.tar.gz")

build() {
	cd "${srcdir}/gdbm-${pkgver}"

	./configure --prefix=/usr --enable-libgdbm-compat

	make
}

package() {
	cd "${srcdir}/gdbm-${pkgver}"

	make install DESTDIR="${pkgdir}"
}

sha512sums=('fdb777f5b415b0578ea46b4a6fc70151a04a3568e2f2c8c0219ccad8c0d34abb773d80d5d7a06462ad1a3d6c1f3b5b7ab25f9de4f43ccf05c58db05f9a2906ad')

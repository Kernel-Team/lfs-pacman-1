pkgname=gdbm
pkgver=1.16
pkgrel=1
pkgdesc="GNU database library"
arch=('x86_64')
url="https://www.gnu.org/software/gdbm/"
license=('GPL3')
#depends=()
source=("https://ftp.gnu.org/gnu/gdbm/gdbm-${pkgver}.tar.gz")

build() {
	cd "${srcdir}/gdbm-${pkgver}"

	./configure --prefix=/usr --enable-libgdbm-compat

	make
}

package() {
	cd "${srcdir}/gdbm-${pkgver}"

	make install DESTDIR="${pkgdir}"
}

sha512sums=('97f4f37fd95cee4dd5c9c3f42ea3aecaa28016e90fe0e168a9d8c38e92602221f946c273bb249d508200249701c6403d0f9351460485dfae34a3bf625de59d2c')

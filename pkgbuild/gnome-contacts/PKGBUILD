pkgname=gnome-contacts
pkgver=3.25.4
pkgrel=1
pkgdesc="Contacts Manager for GNOME"
arch=('x86_64')
url="https://wiki.gnome.org/Apps/Contacts"
license=('GPL2')
#depends=()
source=("https://download.gnome.org/sources/gnome-contacts/${pkgver:0:4}/gnome-contacts-${pkgver}.tar.xz")

build() {
	cd "${srcdir}/gnome-contacts-${pkgver}"

	./configure --prefix=/usr

	make
}

package() {
	cd "${srcdir}/gnome-contacts-${pkgver}"

	make install DESTDIR="${pkgdir}"
}

sha512sums=('01be7af4d03e5b9bc0745cc6acb06a90936dd4dd1d8d55ff9e04ddf13c0c4ad816dab58be1f7f13debd8d09041dca10180f05aeddddd877e89030451bc79123b')

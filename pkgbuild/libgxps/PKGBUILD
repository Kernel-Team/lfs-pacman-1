pkgname=libgxps
pkgver=0.2.5
pkgrel=1
pkgdesc="XPS Documents library"
arch=('x86_64')
url="https://wiki.gnome.org/Projects/libgxps"
license=('GPL2')
#depends=()
source=("https://download.gnome.org/sources/libgxps/${pkgver:0:3}/libgxps-${pkgver}.tar.xz")

build() {
	cd "${srcdir}/libgxps-${pkgver}"

	./configure --prefix=/usr --disable-compile-warnings

	make
}

package() {
	cd "${srcdir}/libgxps-${pkgver}"

	make install DESTDIR="${pkgdir}"
}

sha512sums=('f449c4cb989910797c84614ce9f141d4e9adcda0c18f309f656de397ba5deabfa9b4c558c81be7f52bc44c048c401616775989cd1ff9408eeea44f46ef56635b')

pkgname=python2-typing
pkgver=3.6.2
pkgrel=1
pkgdesc="Type Hints for Python"
arch=('x86_64')
url="https://pypi.python.org/pypi/typing/"
license=('PSF')
#depends=()
source=("https://files.pythonhosted.org/packages/source/t/typing/typing-${pkgver}.tar.gz")

build() {
	cd "${srcdir}/typing-${pkgver}"

	python2 setup.py build
}

package() {
	cd "${srcdir}/typing-${pkgver}"

	python2 setup.py install --root="${pkgdir}" --optimize=1
}

sha512sums=('a975980e7ba6ec3e15c938336877071785bec7212443da079770348ab9441c8d918cd806e85ebea8af66a5d94f684566ea07b55e7623ab11e7d922c8c3dfe981')

pkgname=python2-typing
pkgver=3.6.1
pkgrel=1
pkgdesc="Type Hints for Python"
arch=('x86_64')
url="https://pypi.python.org/pypi/typing/"
license=('PSF')
#depends=()
source=("https://files.pythonhosted.org/packages/source/t/typing/typing-${pkgver}.tar.gz")

build() {
	cd "${srcdir}/typing-${pkgver}"

	python2 setup.py build
}

package() {
	cd "${srcdir}/typing-${pkgver}"

	python2 setup.py install --root="${pkgdir}" --optimize=1
}

sha512sums=('37881848a85545329360a5ddaae06deadefa36725c19c6fdfbbaeac1c5b18ad556d39bd57ea3eb4f9f609e16c0a1bd00dc076689d8c06315549594805e53c3eb')

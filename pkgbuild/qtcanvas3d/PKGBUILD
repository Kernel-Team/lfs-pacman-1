pkgname=qtcanvas3d
pkgver=5.9.0
pkgrel=1
pkgdesc="A JavaScript 3D rendering API for Qt Quick"
arch=('x86_64')
url="https://www.qt.io/"
license=('GPL3' 'LGPL3' 'FDL' 'custom')
#depends=()
source=("https://download.qt.io/official_releases/qt/${pkgver:0:3}/${pkgver}/submodules/qtcanvas3d-opensource-src-${pkgver}.tar.xz")

build() {
	cd "${srcdir}/qtcanvas3d-opensource-src-${pkgver}"

	qmake
	make
}

package() {
	cd "${srcdir}/qtcanvas3d-opensource-src-${pkgver}"

	make install INSTALL_ROOT="${pkgdir}"
}

sha512sums=('9e977e28f23574f06e162dcc756ed2fcf700c935a02ef5eab2fd014da02fed0347cac55a92d757a3ad4e478a8955c0c65490068f62d079a3d3d699f84c4487a5')

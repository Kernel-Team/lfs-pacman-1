pkgname=webrtc-audio-processing
pkgver=0.3.1
pkgrel=1
pkgdesc="AudioProcessing library based on Google's implementation of WebRTC"
arch=('x86_64')
url="https://freedesktop.org/software/pulseaudio/webrtc-audio-processing/"
license=('custom')
#depends=()
source=("https://www.freedesktop.org/software/pulseaudio/webrtc-audio-processing/webrtc-audio-processing-${pkgver}.tar.xz")

build() {
	cd "${srcdir}/webrtc-audio-processing-${pkgver}"

	./configure --prefix=/usr

	make
}

package() {
	cd "${srcdir}/webrtc-audio-processing-${pkgver}"

	make install DESTDIR="${pkgdir}"
}

sha512sums=('1c7a2d16f7f6c03cf6d60405d0dcd224caae6e80c9c4d43f8373bad2446affcdf49a02efb0085387328289aa79c8981dcaedff876cde55be9602dbde9c3f440b')

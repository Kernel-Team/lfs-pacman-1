pkgname=xbitmaps
pkgver=1.1.1
pkgrel=1
pkgdesc="X.org Bitmap files"
arch=('x86_64')
url="https://www.x.org/wiki/"
license=('custom')
#depends=()
source=("https://www.x.org/releases/individual/data/xbitmaps-${pkgver}.tar.bz2")

build() {
	cd "${srcdir}/xbitmaps-${pkgver}"

	./configure --prefix=/usr

	make
}

package() {
	cd "${srcdir}/xbitmaps-${pkgver}"

	make install DESTDIR="${pkgdir}"
}

sha512sums=('172a74014e699e5f295ae726a77f1669baf4ac00d64a138b77e5502102b2a02c10127f35ccea5b6af5479d17f60a6bb63ac0f943fd071ab8b396d43ef314ae61')

pkgname=xorg-xwininfo
pkgver=1.1.3
pkgrel=1
pkgdesc="Command-line utility to print information about windows on an X server"
arch=('x86_64')
url="https://www.x.org/wiki/"
license=('custom')
#depends=()
source=("https://www.x.org/releases/individual/app/xwininfo-${pkgver}.tar.bz2")

build() {
	cd "${srcdir}/xwininfo-${pkgver}"

	./configure --prefix=/usr

	make
}

package() {
	cd "${srcdir}/xwininfo-${pkgver}"

	make install DESTDIR="${pkgdir}"
}

sha512sums=('b4c3c72de157d33e309694be96dcafabb3240c1883fb3caf44843cba8b90fe09496eb4ab061eba9107115d76ca59a8ec660e11ff968e422ab35cd5e1749ca466')
